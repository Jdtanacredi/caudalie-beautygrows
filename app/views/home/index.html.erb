<div class="fb-tab-content">
  <div id="beautygrows-header">
    <%= image_tag('BeautyGrowsHeader_v2.jpg') %>
  </div>
  <div id="beautygrows-gallery">
    <% @photos.each_with_index do |photo, i| %>
      <div class="instaphoto p<%= i %>">
        <%= image_tag photo.standard_url %>
      </div>
    <% end %>
  </div>
</div>

